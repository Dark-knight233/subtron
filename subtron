sleep 2
printf "\n\n${WHITE}[!] Enter domain to Enumerate : "
read -r url

printf "${BLUE}\n[+] Subdomain Enumeration started!\n"

printf "${GREEN}\n[+] Enumerating subdomains ..."
curl -s "https://crt.sh/?q=$url" | grep -Po "(([\w.-]*)\.([\w]*)\.([A-z]))\w+" | sort -u > sub1

sort sub1  | uniq | tee $url-subdomains
rm sub*

sleep 2

echo ""
echo "[+] Finished!!!"
echo ""
